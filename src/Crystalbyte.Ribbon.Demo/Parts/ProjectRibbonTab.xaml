<ui:RibbonTab x:Class="Crystalbyte.Ribbon.Demo.Parts.ProjectRibbonTab"
              xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
              xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
              xmlns:ui="clr-namespace:Crystalbyte.UI;assembly=Crystalbyte.Ribbon"
              Header="PROJECT"
              d:DesignHeight="300"
              d:DesignWidth="300"
              mc:Ignorable="d">
    <ui:RibbonPage>
        <ui:RibbonGroup Header="Actions">
            <ui:RibbonButton Command="{StaticResource DummyCommand}"
                             Content="Add"
                             ImageSource="../Assets/add.png" />
            <ui:RibbonButton Command="{StaticResource DummyCommand}"
                             Content="Remove"
                             ImageSource="../Assets/delete.png" />
        </ui:RibbonGroup>
    </ui:RibbonPage>
</ui:RibbonTab>
